{
  "name": "@reactants/ui",
  "version": "0.12.0",
  "main": "dist/components/index.js",
  "module": "dist/components/index.js",
  "types": "dist/components/index.d.ts",
  "license": "MIT",
  "description": "Alles React UI library.",
  "scripts": {
    "dev": "next dev",
    "docs-build": "yarn docs-collect && next build",
    "docs-start": "next start",
    "clear": "rm -rf dist",
    "build": "yarn run clear && rollup -c scripts/rollup.config.js && tsc -p scripts/tsconfig.json && cp components/styles/global.css dist/global.css",
    "release": "yarn run build && yarn publish --access public --non-interactive"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "styled-jsx": "^3.2.5"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-typescript": "^4.1.1",
    "@types/classnames": "^2.2.10",
    "@types/node": "^13.13.4",
    "@types/react": "^16.9.34",
    "@types/styled-jsx": "^2.2.8",
    "@wessberg/rollup-plugin-ts": "^1.2.24",
    "next": "^9.3.6",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-feather": "^2.0.8",
    "rollup": "^2.8.2",
    "rollup-plugin-postcss": "^3.1.1",
    "tslib": "^1.11.2",
    "typescript": "^3.8.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript",
      [
        "@babel/preset-react",
        {
          "useBuiltIns": true
        }
      ],
      [
        "@babel/preset-env"
      ]
    ]
  }
}

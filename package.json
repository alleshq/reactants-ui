{
  "name": "@reactants/ui",
  "version": "0.11.1",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "description": "Alles React UI library.",
  "scripts": {
    "dev": "next dev",
    "docs-build": "yarn docs-collect && next build",
    "docs-start": "next start",
    "clear": "rm -rf dist",
    "build": "yarn run clear && webpack --config scripts/webpack.config.js && tsc -p ./scripts && cp components/styles/global.css dist/global.css",
    "release": "yarn run build && yarn publish --access public --non-interactive"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "styled-jsx": "^3.2.5"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/classnames": "^2.2.10",
    "@types/node": "^13.13.4",
    "@types/react": "^16.9.34",
    "@types/styled-jsx": "^2.2.8",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "fs-extra": "^9.0.0",
    "next": "^9.3.6",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-feather": "^2.0.8",
    "style-loader": "^1.2.1",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "peerDependencies": {}
}
